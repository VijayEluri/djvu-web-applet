<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>javadjvu</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <name>javadjvu</name>
            <folders>
                <source-folder>
                    <label>javadjvu</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>src</label>
                    <type>java</type>
                    <location>src</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>doc</label>
                    <type>doc_root</type>
                    <location>doc</location>
                </source-folder>
                <source-folder>
                    <label>WEB-INF</label>
                    <type>web_inf</type>
                    <location>doc/WEB-INF</location>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>release.jarframe</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>release.jarframe</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="javadoc">
                    <target>doc</target>
                </action>
                <action name="test">
                    <target>test</target>
                </action>
                <action name="run">
                    <target>test</target>
                </action>
                <action name="debug">
                    <target>debug-nb</target>
                </action>
                <action name="redeploy">
                    <target>package</target>
                </action>
            </ide-actions>
            <export>
                <type>folder</type>
                <location>dist</location>
                <build-target>release.jarframe</build-target>
            </export>
            <view>
                <items>
                    <source-folder style="tree">
                        <label>Web Pages</label>
                        <location>doc</location>
                    </source-folder>
                    <source-folder style="tree">
                        <label>WEB-INF Content</label>
                        <location>doc/WEB-INF</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>src</label>
                        <location>src</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <ide-action name="javadoc"/>
                    <ide-action name="test"/>
                    <ide-action name="run"/>
                    <ide-action name="debug"/>
                    <ide-action name="redeploy"/>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/1">
            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">src/locale</classpath>
                <built-to>dist</built-to>
                <source-level>1.3</source-level>
            </compilation-unit>
        </java-data>
        <web-data xmlns="http://www.netbeans.org/ns/freeform-project-web/1">
            <web-module>
                <doc-root>doc</doc-root>
                <classpath>src/locale</classpath>
                <context-path/>
                <j2ee-spec-level>1.3</j2ee-spec-level>
            </web-module>
        </web-data>
    </configuration>
</project>
